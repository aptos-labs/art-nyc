{
  "name": "aptos-office-art",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://art.aptoslabs.com/",
  "scripts": {
    "prepare": "panda codegen",
    "start": "vite --port 3000",
    "build": "tsc --noEmit && vite build",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "fmt": "prettier --write 'src/**/*.(ts|tsx)'",
    "ts:check": "tsc --noEmit",
    "artifactregistry-login": "pnpm dlx google-artifactregistry-auth"
  },
  "dependencies": {
    "@aptos-internal/design-system-web": "0.2.0-beta.16",
    "@aptos-labs/ts-sdk": "=1.13.3",
    "@aptos-labs/aptos-fee-payer-client": "^0.0.7",
    "@aptos-labs/wallet-adapter-react": "^3.7.0",
    "@tanstack/react-query": "^5.32.0",
    "@thalalabs/surf": "^1.2.0",
    "axios": "^1.7.9",
    "buffer": "^6.0.3",
    "petra-plugin-wallet-adapter": "^0.4.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.2"
  },
  "devDependencies": {
    "@pandacss/dev": "^0.46.1",
    "@types/node": "^20.12.12",
    "@types/react": "^18.3.9",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vitejs/plugin-react": "^4.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-import": "^2.30.0",
    "postcss": "^8.4.47",
    "postcss-import": "^16.1.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.2",
    "vite": "^5.4.8"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "no-throw-literal": "off",
      "no-multi-str": "off",
      "no-empty-pattern": "off"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
